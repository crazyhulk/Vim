{
  "Class": {
    "prefix": ["cllass"],
    "body": [
      "/**",
      " * @author ${VIM:\\$USER}",
      " */",
      "class $1 ${2:extends ${3:Parent} }{",
      "\tconstructor() {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Class definition template."
  },
  "iferr": {
    "prefix": ["iferr"],
    "body": [
      "if err != nil {",
      "\treturn",
      "}"
    ],
    "description": "if err != nil..."
  },
  "Function": {
    "prefix": ["func"],
    "body": [ 
      "// $1 ",
      "func $1($2) (err error){",
      "\treturn",
      "}"
    ],
    "description": "func new ..."
  }

}
